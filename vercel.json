{
  "version": 2,
  
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.7.3"
      }
    },
  
  "routes": [
    {
      "src": "/api/(.*)",        
      "dest": "/api/index.php"      
    }
  ],
  "build": {
    "command1":"composer dump-autoload",
    "command": "composer install"
  }
}
